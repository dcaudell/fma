<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <description>Startup servlet context listener</description>
        <listener-class>com.xtrafe.server.Startup</listener-class>
    </listener>
    <servlet>
        <servlet-name>CompanyData</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.CompanyData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CompanyData</servlet-name>
        <url-pattern>/CompanyData</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>

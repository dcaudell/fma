<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <description>Startup servlet context listener</description>
        <listener-class>com.xtrafe.server.Startup</listener-class>
    </listener>
    <servlet>
        <servlet-name>SymbolListing</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.SymbolListing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StockListing</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.StockListing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StartTickDaemon</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.StartTickDaemon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StopTickDaemon</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.StopTickDaemon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EbayQuery</servlet-name>
        <servlet-class>com.xtrafe.fma.server.servlets.EbayQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SymbolListing</servlet-name>
        <url-pattern>/SymbolListing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StockListing</servlet-name>
        <url-pattern>/StockListing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StartTickDaemon</servlet-name>
        <url-pattern>/StartTickDaemon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StopTickDaemon</servlet-name>
        <url-pattern>/StopTickDaemon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EbayQuery</servlet-name>
        <url-pattern>/EbayQuery</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
